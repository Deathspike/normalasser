{
  "author": "Roel van Uden",
  "description": "Extracts ssa/ass subtitle(s) from MKV videos and rescale/resize subtitles.",
  "license": "MIT",
  "main": "dist",
  "name": "normalize-ssa",
  "repository": "git://github.com/Deathspike/normalize-ssa",
  "types": "dist",
  "version": "1.0.2",
  "keywords": [
    "ssa",
    "ass",
    "normalize",
    "resize",
    "rescale"
  ],
  "bin": {
    "normalize-ssa": "./bin/normalize-ssa"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "ass-compiler": "0.1.2",
    "commander": "6.1.0",
    "fs-extra": "9.1.0",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.7",
    "@types/node": "14.11.5",
    "rimraf": "3.0.2",
    "typescript": "4.0.3"
  },
  "scripts": {
    "prepare": "rimraf dist && tsc",
    "postinstall": "node postinstall",
    "start": "node ."
  }
}
